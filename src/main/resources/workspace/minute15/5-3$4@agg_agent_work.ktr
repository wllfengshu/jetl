	tmp.domain,
	tmp.agent_no,
	ifnull(u.org_id,'') as org_id,
	ifnull(u.user_name,'') as agent_name,
	ifnull(q.queue_no,'') as queue_no,
	ifnull(tmp.outbound_call_num,0) as outbound_call_num,
	ifnull(tmp.outbound_talk_num,0) as outbound_talk_num,
	(ifnull(s.aux,0)+ifnull(s.acw,0)+ifnull(s.available,0)+ifnull(s.allocate,0)+ifnull(s.talk_length,0)+ifnull(s.ring_length,0)) as total_login_length,
	(ifnull(tmp.internal_talk_length,0)+ifnull(tmp.outbound_talk_length,0)) as total_talk_length,
	ifnull(tmp.internal_talk_length,0)as internal_talk_length,
	ifnull(tmp.outbound_talk_length,0)as outbound_talk_length,
	0 as ask_length,
	(ifnull(tmp.inbound_ring_length,0)+ifnull(tmp.outbound_ring_length,0)) as total_ring_length,
	ifnull(tmp.inbound_call_num,0)as inbound_call_num,
	ifnull(tmp.inbound_talk_length,0)as inbound_talk_length,
	ifnull(tmp.outbound_ring_length,0)as outbound_ring_length,
	ifnull(s.acw,0) as acw,
	0 as total_length,
	ifnull(tmp2.login_time,'${beginTime}') as login_time,
	0 as aux_0,
	0 as aux_1,
	0 as aux_2,
	0 as aux_3,
	0 as aux_4,
	ifnull(tmp.inbound_talk_num,0) as inbound_talk_num,
	ifnull(tmp2.hold_length,0) as hold_length,
	ifnull(tmp.internal_talk_num,0) as internal_talk_num
	FROM(
	SELECT
		a.time_id as time_id,
		a.domain,
		a.agent_no as agent_no,
		a.call_num as inbound_call_num,
		0 as internal_call_num,
		0 as outbound_call_num,
		a.talk_num as inbound_talk_num,
		0 as internal_talk_num,
		0 as outbound_talk_num,
		a.ring_length as inbound_ring_length,
		0 as internal_ring_length,
		0 as outbound_ring_length,
		a.talk_length as inbound_talk_length,
		0 as internal_talk_length,
		0 as outbound_talk_length
		FROM agg_agent_detail a
		where (
				a.time_id BETWEEN '${beginTimeId}' AND '${endTimeId}'
				AND a.domain &lt;&gt; ''
				AND a.agent_no &lt;&gt; ''
				AND a.call_type = 0
		)
		
		UNION ALL
		
		SELECT
		a.time_id as time_id,
		a.domain,
		a.agent_no as agent_no,
		0 as inbound_call_num,
		a.call_num as internal_call_num,
		0 as outbound_call_num,
		0 as inbound_talk_num,
		a.talk_num as internal_talk_num,
		0 as outbound_talk_num,
		0 as inbound_ring_length,
		a.ring_length as internal_ring_length,
		0 as outbound_ring_length,
		0 as inbound_talk_length,
		a.talk_length as internal_talk_length,
		0 as outbound_talk_length
		FROM agg_agent_detail a
		where (
				a.time_id BETWEEN '${beginTimeId}' AND '${endTimeId}'
				AND a.domain &lt;&gt; ''
				AND a.agent_no &lt;&gt; ''
				AND a.call_type = 3
		)
		
		UNION ALL
		
		SELECT
		a.time_id as time_id,
		a.domain,
		a.agent_no as agent_no,
		0 as inbound_call_num,
		0 as internal_call_num,
		SUM(a.call_num) as outbound_call_num,
		0 as inbound_talk_num,
		0 as internal_talk_num,
		SUM(a.talk_num) as outbound_talk_num,
		0 as inbound_ring_length,
		0 as internal_ring_length,
		SUM(a.ring_length) as outbound_ring_length,
		0 as inbound_talk_length,
		0 as internal_talk_length,
		SUM(a.talk_length) as outbound_talk_length
		FROM agg_agent_detail a
		where (
				a.time_id BETWEEN '${beginTimeId}' AND '${endTimeId}'
				AND a.domain &lt;&gt; ''
				AND a.agent_no &lt;&gt; ''
				AND a.call_type not in (0,3)
		) 
		GROUP BY time_id,a.domain,agent_no,inbound_call_num,internal_call_num,inbound_talk_num,internal_talk_num,inbound_ring_length,internal_ring_length,inbound_talk_length,internal_talk_length
	) tmp 
	LEFT JOIN t_user u ON tmp.agent_no = concat(u.agent_no,'@',u.namespace)
	LEFT JOIN agg_agent_status s ON tmp.agent_no = s.agent_no AND tmp.time_id = s.time_id
	LEFT JOIN (
		SELECT CONCAT(LEFT(s.time_id, 8), '0000') AS time_id, s.agent_id, 
		sum(case when s.agent_status='Hold' then s.length else 0 end) as hold_length,
		min(case when s.agent_status='Login' then s.begin_time else null end) as login_time
		FROM t_agent_status s 
		WHERE(
			s.time_id BETWEEN '${beginTimeId}' AND '${endTimeId}' 
			AND s.agent_id &lt;&gt; ''
		)
		GROUP BY time_id, s.agent_id
	)tmp2 ON tmp.agent_no = tmp2.agent_id AND tmp.time_id = tmp2.time_id
	LEFT JOIN dim_agent_queue q ON q.agent_no = tmp.agent_no</sql>
