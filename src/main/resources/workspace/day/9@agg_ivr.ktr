    <connection>输入数据库</connection>
    <sql>SELECT
CONCAT(LEFT(c.time_id, 8), '0000') AS time_id,
c.domain,
c.first_ivr_no as ivr_no,
count(c.call_id) as call_num,
ifnull(tmp_s.talk_num,0) as talk_num,
ifnull(tmp_t.transfer_person_num,0) as transfer_person_num
FROM tb_call c
LEFT JOIN (
    SELECT
      left(s.time_in,8) as time_in,
      s.ivr_no,
      count(s.call_id)as talk_num
    FROM tb_segment_ivr s
    WHERE s.time_in BETWEEN '${beginTime}'
		AND '${endTime}'
    AND s.ivr_no &lt;&gt; ''
    GROUP BY
      left(s.time_in,8),
      s.ivr_no
)tmp_s ON left(c.time_in,8)=tmp_s.time_in and c.first_ivr_no=tmp_s.ivr_no
LEFT JOIN (
    SELECT
      left(t.time_in,8) as time_in,
      t.ivr_no,
      count(t.call_id)as transfer_person_num
    FROM tb_trace_ivr t
    WHERE t.time_in BETWEEN '${beginTime}'
		AND '${endTime}'
    AND t.ivr_no &lt;&gt; ''
    AND t.tag1='转人工' or t.tag2='转人工' or t.tag3='转人工'
    GROUP BY
      left(t.time_in,8),
      t.ivr_no
)tmp_t ON left(c.time_in,8)=tmp_t.time_in and c.first_ivr_no=tmp_t.ivr_no
WHERE
(
		c.time_in BETWEEN '${beginTime}'
		AND '${endTime}'
		AND c.domain &lt;&gt; ''
    AND c.first_ivr_no &lt;&gt; ''
)
GROUP BY
CONCAT(LEFT(c.time_id, 8), '0000'),
c.domain,
c.first_ivr_no,
tmp_s.talk_num,
tmp_t.transfer_person_num

