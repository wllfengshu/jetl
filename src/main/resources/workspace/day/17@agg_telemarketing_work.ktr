tmp.domain,
tmp.agent_no,
ifnull(u.user_name,'') as user_name,
ifnull(tmp.queue_no,'') as queue_no,
ifnull(o.org_name,'') as org_name, 
ifnull(sum(call_num),0) as call_num,
ifnull((sum(tmp.inbound_talk_num) + sum(tmp.outbound_talk_num) + sum(tmp.manual_talk_num)),0) as talk_num,
ifnull(sum(tmp.inbound_talk_num),0) as inbound_talk_num,
ifnull(sum(tmp.outbound_talk_num),0) as outbound_talk_num,
ifnull(sum(tmp.manual_talk_num),0) as manual_talk_num,
ifnull(sum(a1.login_time),0) as login_keep_time,
ifnull(sum(a1.available_time),0) as available_keep_time,
ifnull(sum(a1.aux_time),0) as aux_keep_time,
ifnull(sum(tmp.talk_length),0) as talk_length,
ifnull(sum(a1.acw_time),0) as acw_length,
ifnull((sum(tmp.talk_length) + sum(a1.acw_time)),0) as talk_handle_length,
ifnull(sum(tmp.manual_handle_length),0) as manual_handle_length,
ifnull((sum(tmp.talk_length) + sum(a1.acw_time))/(sum(tmp.inbound_talk_num) + sum(tmp.outbound_talk_num) + sum(tmp.manual_talk_num)),0) as handle_call_percentage,
ifnull(sum(a1.available_time)/count(DISTINCT tmp.agent_no),0) as avg_available_time,
ifnull(sum(tmp.talk_length)/(sum(tmp.inbound_talk_num) + sum(tmp.outbound_talk_num) + sum(tmp.manual_talk_num)),0) as avg_talk_length,
ifnull(sum(a1.acw_time)/(sum(tmp.inbound_talk_num) + sum(tmp.outbound_talk_num) + sum(tmp.manual_talk_num)),0) as avg_acw_length,
ifnull(sum(tmp.manual_handle_length)/sum(tmp.manual_talk_num),0) as avg_manual_handle_time,
(1-ifnull(sum(a1.aux_time)/sum(a1.login_time),0)) as rate_time_available
FROM(
SELECT 
CONCAT(LEFT(c.time_id, 8), '0000') AS time_id,
c.domain as domain,
c.first_agent_no as agent_no,
c.last_queue_no as queue_no,
0 as call_num,
sum(case when c.first_station_answer_time &lt;&gt; '1990-01-01 00:00:00.000' then 1 else 0 end) as inbound_talk_num,
0 as outbound_talk_num,
0 as manual_talk_num,
sum(c.talk_length) as talk_length,
0 as manual_handle_length
FROM tb_call c
WHERE(
c.time_in BETWEEN '${beginTime}'
			AND '${endTime}'
			AND c.domain &lt;&gt; ''
			AND c.first_agent_no &lt;&gt; ''
			AND c.call_type = 0
)
GROUP BY CONCAT(LEFT(c.time_id, 8), '0000'),
c.domain,
c.first_agent_no,
c.last_queue_no

UNION ALL

SELECT CONCAT(LEFT(c.time_id, 8), '0000') AS time_id,
c.domain as domain,
c.first_agent_no as agent_no,
c.last_queue_no as queue_no,
COUNT(c.call_id) as call_num,
0 as inbound_talk_num,
sum(case when c.customer_answer_time &lt;&gt; '1990-01-01 00:00:00.000' then 1 else 0 end) as outbound_talk_num,
0 as manual_talk_num,
sum(c.talk_length) as talk_length,
0 as manual_handle_length
FROM tb_call c
WHERE(
c.time_in BETWEEN '${beginTime}'
			AND '${endTime}'
			AND c.domain &lt;&gt; ''
			AND c.first_agent_no &lt;&gt; ''
			AND c.call_type in (4,5,7) 
)
GROUP BY CONCAT(LEFT(c.time_id, 8), '0000'),
c.domain,
c.first_agent_no,
c.last_queue_no

UNION ALL

SELECT CONCAT(LEFT(c.time_id, 8), '0000') AS time_id,
c.domain as domain,
c.first_agent_no as agent_no,
c.last_queue_no as queue_no,
COUNT(c.call_id) as call_num,
0 as inbound_talk_num,
0 as outbound_talk_num,
sum(case when c.customer_answer_time &lt;&gt; '1990-01-01 00:00:00.000' then 1 else 0 end) as manual_talk_num,
sum(c.talk_length) as talk_length,
c.call_length as manual_handle_length
FROM tb_call c
WHERE(
c.time_in BETWEEN '${beginTime}'
			AND '${endTime}'
			AND c.domain &lt;&gt; ''
			AND c.first_agent_no &lt;&gt; ''
			AND c.call_type = 1
)
GROUP BY CONCAT(LEFT(c.time_id, 8), '0000'),
c.domain,
c.first_agent_no,
c.last_queue_no,
c.call_length
) tmp
LEFT JOIN t_user u ON tmp.agent_no = CONCAT(u.agent_no,'@',u.namespace)
LEFT JOIN t_org o ON u.org_id = o.id
LEFT JOIN (
	SELECT a.agent_id, CONCAT(LEFT(a.time_id, 8), '0000') as time_id,
	SUM(case when a.agent_status='Login' then a.length else 0 end) as login_time,
	SUM(case when a.agent_status='Available' then a.length else 0 end) as available_time,
	SUM(case when a.agent_status='Aux' then a.length else 0 end) as aux_time,
	SUM(case when a.agent_status='Acw' then a.length else 0 end) as acw_time
	FROM t_agent_status a
	GROUP BY a.agent_id, CONCAT(LEFT(a.time_id, 8), '0000')
)a1 ON tmp.agent_no = a1.agent_id and tmp.time_id = CONCAT(LEFT(a1.time_id, 8), '0000')
GROUP BY tmp.time_id,
tmp.domain,
tmp.agent_no,
u.user_name,
tmp.queue_no,
o.org_name</sql>
