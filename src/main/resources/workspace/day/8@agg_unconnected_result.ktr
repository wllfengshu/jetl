    <connection>输入数据库</connection>
    <sql>SELECT
		CONCAT(LEFT(c.time_id, 8), '0000') AS time_id,
		c.domain,
		r.group_campaign_id,
		r.campaign_id,
		r.batch_id,
        r.batch_name,
        ifnull(a.province,'') AS province,
		ifnull(a.city,'') AS city,
		ifnull(a.isp,'') AS isp,
		c.call_type,
		ct.code_name AS call_type_name,
sum(case when c.result_code=-7 then 1 else 0 end) as result_code_neg7,
sum(case when c.result_code=-5 then 1 else 0 end) as result_code_neg5,
sum(case when c.result_code=-3 then 1 else 0 end) as result_code_neg3,
sum(case when c.result_code=-1 then 1 else 0 end) as result_code_neg1,
sum(case when c.result_code=0 then 1 else 0 end) as result_code0,
sum(case when c.result_code=1 then 1 else 0 end) as result_code1,
sum(case when c.result_code=2 then 1 else 0 end) as result_code2,
sum(case when c.result_code=3 then 1 else 0 end) as result_code3,
sum(case when c.result_code=4 then 1 else 0 end) as result_code4,
sum(case when c.result_code=5 then 1 else 0 end) as result_code5,
sum(case when c.result_code=6 then 1 else 0 end) as result_code6,
sum(case when c.result_code=7 then 1 else 0 end) as result_code7,
sum(case when c.result_code=8 then 1 else 0 end) as result_code8,
sum(case when c.result_code=9 then 1 else 0 end) as result_code9,
sum(case when c.result_code=10 then 1 else 0 end) as result_code10,
sum(case when c.result_code=11 then 1 else 0 end) as result_code11,
sum(case when c.result_code=12 then 1 else 0 end) as result_code12,
sum(case when c.result_code=99 then 1 else 0 end) as result_code99
	FROM tb_call c
	LEFT JOIN tb_aob_result r ON c.call_id=r.call_id
  LEFT JOIN area_code a ON left(c.customer_no,7)=a.phone
  LEFT JOIN call_dictionary ct ON ct.type='CALL_TYPE' and c.call_type = ct.code
	WHERE
	(
			c.time_in BETWEEN '${beginTime}' AND '${endTime}'
			and c.domain&lt;&gt;''
			and c.call_type in (4,5,7)
      and r.time_in BETWEEN '${beginTime}' AND '${endTime}'
		  and r.group_campaign_id&lt;&gt;''
		  and r.campaign_id&lt;&gt;''
		  and r.batch_id&lt;&gt;''
	)
	GROUP BY
		CONCAT(LEFT(c.time_id, 8), '0000'),
		c.domain,
		r.group_campaign_id,
		r.campaign_id,
		r.batch_id,
        r.batch_name,
    a.province,
		a.city,
		a.isp,
		c.call_type,
		ct.code_name
</sql>
